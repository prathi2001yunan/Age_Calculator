<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/calcb"
    tools:context=".MainActivity2">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView1"
        android:layout_width="189dp"
        android:layout_height="243dp"
        android:layout_marginTop="78dp"
        android:layout_marginBottom="337dp"
        android:background="@drawable/icc"
        android:backgroundTint="#4AFFFFFF"
        android:elevation="40dp"
        app:cardCornerRadius="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082">

        <TextView
            android:id="@+id/textView45"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:layout_marginTop="160dp"
            android:background="#E91E63"
            android:backgroundTint="#5EF3085C"
            android:drawableRight="@drawable/baseline_calendar_today_24"
            android:fontFamily="@font/acme"
            android:text="SELECT TODAY'S DATE"
            android:textColor="#FDFDFD"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/img"
            style="@android:style/Widget.KeyboardView"
            android:layout_width="wrap_content"
            android:layout_height="99dp"
            android:layout_marginLeft="25dp"

            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/cc" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="182dp"
        android:layout_height="243dp"
        android:layout_marginTop="78dp"
        android:layout_marginBottom="337dp"
        android:background="@drawable/icc"
        android:backgroundTint="#4AFFFFFF"
        android:elevation="40dp"
        app:cardCornerRadius="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.969"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:layout_marginTop="160dp"
            android:background="#E91E63"
            android:backgroundTint="#5EF3085C"
            android:drawableRight="@drawable/baseline_calendar_today_24"
            android:fontFamily="@font/acme"
            android:text="SELECT BDAY'S DATE"
            android:textColor="#F6F1F1"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/imageButton5"
            style="@android:style/Widget.KeyboardView"
            android:layout_width="wrap_content"
            android:layout_height="99dp"
            android:layout_marginLeft="25dp"

            android:layout_marginTop="30dp"

            app:srcCompat="@drawable/cc" />

    </androidx.cardview.widget.CardView>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button3"
        android:layout_width="179dp"
        android:layout_height="67dp"
        android:layout_marginTop="44dp"
        android:fontFamily="serif-monospace"
        android:text="CALCULATE"
        android:textAlignment="center"
        android:textSize="20sp"
        app:backgroundTint="#B2673AB7"
        app:cornerRadius="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:id="@+id/hf"
        android:layout_width="366dp"
        android:layout_height="241dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="148dp"
        android:backgroundTint="#63FFFFFF"
        app:cardBackgroundColor="#75FFFFFF"
        app:cardCornerRadius="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="0.35000002">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#F44336"
            android:backgroundTint="#B5F44336"

            android:drawableLeft="@drawable/baseline_person_24"
            android:fontFamily="@font/atomic_age"
            android:textColor="#FDFDFD"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="50dp"
            android:background="#F44336"
            android:backgroundTint="#B5F44336"
            android:drawableLeft="@drawable/baseline_person_24"
            android:fontFamily="@font/atomic_age"
            android:textColor="#F1E9E9"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="100dp"
            android:background="#F44336"
            android:backgroundTint="#B5F44336"
            android:drawableLeft="@drawable/baseline_person_24"
            android:elevation="20dp"
            android:fontFamily="@font/atomic_age"
            android:textColor="#F6EFEF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="150dp"
            android:background="#F44336"
            android:backgroundTint="#B5F44336"
            android:drawableLeft="@drawable/baseline_person_24"
            android:elevation="20dp"
            android:fontFamily="@font/atomic_age"
            android:textColor="#F6EFEF"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="200dp"
            android:background="#F44336"
            android:backgroundTint="#B5F44336"
            android:drawableLeft="@drawable/baseline_person_24"
            android:elevation="20dp"
            android:fontFamily="@font/atomic_age"
            android:textColor="#F6EFEF"
            android:textSize="20sp" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>